- 
    let menuLinks = [
        {
            text: 'WHY US',
            active: false
        },
        {
            text: 'VPN APPS',
            active: false
        },
        {
            text: 'SUPPORT',
            active: false
        },
        {
            text: 'Resources',
            active: true
        }
    ]

header.header 
    .container 
        .header__wrapper
            button.header__burger 
                    img(src="assets/img/burger_icon.svg", alt="Burger Icon")
            .header__logo Company Name
            ul.header__menu 
                each link in menuLinks
                    li(class=link.active ? 'header__menu-list active' : 'header__menu-list')
                        a.header__menu-link(href="#") #{link.text}
            .header__buttons 
                button.header__login.btn.btn__outline LOG IN
                button.header__signup.btn.btn__fill GET STARTED